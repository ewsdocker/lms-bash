<lms>

	<declarations name="lmscli_" >

		<declare name="shellParam" type="associative" />

		<declare name="optDebug"  type="integer">0</declare>
		<declare name="optHelp"	  type="integer">0</declare>
		<declare name="optQuiet"  type="integer">0</declare>

		<declare parent="shellParam" type="element"  name="debug" >optDebug</declare>
		<declare parent="shellParam" type="element"  name="help"  >optHelp</declare>
		<declare parent="shellParam" type="element"  name="quiet" >optSilent</declare>

	</declarations>

	<declarations name="lmsins_" >

		<declare name="reduceQuote" type="integer">1</declare>

	</declarations>

	<ErrorMsgs>

		<ErrorCode name="AppBkup">
			<message>Source backup failed.</message>
		</ErrorCode>

		<ErrorCode name="TarError">
			<message>'tar' request failed.</message>
		</ErrorCode>

		<ErrorCode name="ExtractError">
			<message>Extract error.</message>
		</ErrorCode>

		<ErrorCode name="InstallError">
			<message>Install error.</message>
		</ErrorCode>

		<ErrorCode name="BackupError">
			<message>Backup error.</message>
		</ErrorCode>

	</ErrorMsgs>
</lms>


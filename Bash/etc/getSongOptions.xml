<lms>

	<declarations name="lmscli_" >

		<declare name="shellParameters" type="associative" />

		<declare name="optDebug"  type="integer">0</declare>
		<declare name="optHelp"	  type="integer">0</declare>
		<declare name="optQuiet"  type="integer">0</declare>

		<declare name="optAlter"  type="string">-</declare>
		<declare name="optBase"   type="string">/media/sf_Shared/MBroadcast/butt/djmaster/</declare>
		<declare name="optPlay"   type="integer">10</declare>
		<declare name="optRun"    type="integer">30</declare>
		<declare name="optSleep"  type="integer">5</declare>
		<declare name="optReduce" type="integer">1</declare>

		<declare parent="shellParameters" type="element"  name="debug" >optDebug</declare>
		<declare parent="shellParameters" type="element"  name="help"  >optHelp</declare>
		<declare parent="shellParameters" type="element"  name="quiet" >optSilent</declare>

		<declare parent="shellParameters" type="element"  name="base"  >optBase</declare>
		<declare parent="shellParameters" type="element"  name="run"   >optRun</declare>
		<declare parent="shellParameters" type="element"  name="play"  >optPlay</declare>
		<declare parent="shellParameters" type="element"  name="sleep" >optSleep</declare>
		<declare parent="shellParameters" type="element"  name="alter" >optAlter</declare>
		<declare parent="shellParameters" type="element"  name="reduce">optReduce</declare>

	</declarations>

	<declarations name="lmssng_" >

		<declare name="fileRoot"           type="string">/media/sf_Shared/MBroadcast/songlists/</declare>

		<declare name="fileCurrent"        type="string">CurrentSong</declare>
		<declare name="fileSongName"       type="string">SongName</declare>
		<declare name="fileSongHost"       type="string">SongHost</declare>

		<declare name="fileRootList"       type="string">/media/sf_Shared/MBroadcast/songlists/djmaster/</declare>
		<declare name="fileListName"       type="string">SongList</declare>

		<declare name="lmssng_reduceQuote" type="integer">1</declare>

	</declarations>

</lms>

